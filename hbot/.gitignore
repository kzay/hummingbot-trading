# ============================================
# Hummingbot Infrastructure - .gitignore
# ============================================

# ---- Secrets - NEVER commit ----
env/.env
env/.env.*
!env/.env.template
.env
!.env.example
*.pem
*.key
*.crt

# ---- Bot runtime data (logs, fills, state) ----
# CSV logs (minute, fills, daily) — high-volume, not for VCS
data/*/logs/**/*.csv
data/*/logs/**/*.wal
data/*/logs/**/*.log
data/*/logs/**/*.log.*
# Bot state snapshots — regenerated on every tick
data/*/logs/**/*.json
# Event journals and desk streams (append-only runtime logs)
data/*/logs/**/*.jsonl
# Keep only conf and scripts
data/*/data/*.sqlite
data/*/data/*.db
data/*/data/*.json
!data/*/conf/.gitkeep
!data/*/conf/**

# Built-in scripts copied from image (not our code)
data/*/scripts/*.py
data/*/scripts/basic/
data/*/scripts/community/
data/*/scripts/utility/
!data/*/scripts/.gitkeep

# ---- Runtime reports (generated by services) ----
# These are ephemeral artifacts — NOT source of truth for the repo.
# Keep only: baseline_counts.json, day2_gate_eval_latest.json,
#   latest.json (per subdir), promotion_gates/latest.*, readiness_decision_latest.*
#
# Daily ops generated markdown snapshots
docs/ops/daily_ops_report_*.md
#
# Timestamped report snapshots (all subdirs)
reports/accounting/accounting_integrity_*T*.json
reports/backtest_regression/backtest_regression_*T*.json
reports/event_store/events_*.jsonl
reports/event_store/integrity_*.json
reports/event_store/source_compare_*T*.json
reports/event_store/baseline_reset_*.json
reports/event_store/baseline_counts_backup_*.json
reports/notrade_validation/notrade_validation_*T*.json
reports/parity/**/*_*T*.json
reports/paper_soak/paper_soak_*T*.json
reports/portfolio_risk/portfolio_risk_*T*.json
reports/portfolio_risk/audit_*.jsonl
reports/promotion_gates/promotion_gates_*T*.json
reports/promotion_gates/strict_cycle_*T*.json
reports/reconciliation/reconciliation_*T*.json
reports/replay_regression/replay_regression_*T*.json
reports/soak/soak_snapshot_*T*.json
reports/post_trade/post_trade_*T*.json
reports/security/secrets_hygiene_*T*.json
reports/strategy_catalog/strategy_catalog_check_*T*.json
reports/coordination/coordination_*T*.json
reports/**/*.jsonl
reports/**/*_20*T*.json
reports/**/*_20*T*.md
reports/**/latest.json
reports/**/latest.md
reports/tests/coverage.json
reports/tests/coverage.xml

# Volatile latest pointers from runtime services
reports/coordination/latest.json
reports/exchange_snapshots/latest.json
reports/kill_switch/latest.json
reports/parity/latest.json
reports/portfolio_risk/latest.json
reports/reconciliation/latest.json
reports/soak/latest.json
reports/event_store/day2_gate_eval_latest.json

# ---- Grafana / Prometheus runtime ----
monitoring/grafana/grafana.db
monitoring/grafana/plugins/
monitoring/prometheus/data/

# ---- Backups ----
backups/*.tar.gz
backups/*.zip
backups/*.sql

# ---- Docker local override ----
docker-compose.override.yml

# ---- OS files ----
.DS_Store
Thumbs.db
desktop.ini

# ---- IDE ----
.vscode/
.idea/
*.swp
*.swo
*~

# ---- Python ----
__pycache__/
*.pyc
*.pyo
.venv/
venv/

